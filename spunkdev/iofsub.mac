        RETURN  SFDP

;************************************************************
        SUBTTL  SFEXP   CHART R66
        DCL     SFEXP
;************************************************************

; R66.1-.2
        OAOS     ,LMIOC

; R66.3
        MOVE    R5,LENG
        CAIN    R5,1
        JRST    SFEXP5

; R66.4
        CALL    FMTERR,<RSJMSG+^D18>

