;
;*****************************************************************
        TITLE   IOFSCN   I/O FORM SCANNER  ("FORM ONLY" SEGMENT)
;*****************************************************************
;
        TWOSEG
        RELOC   ^O400000
        RADIX   10
;
        ENTRY   FRMSCN
;
        EXTERN  IFINIT, IFBLIT, IFBNL, IFINCF, IFMDP, IFCLIT, IFCNL
        EXTERN  IFEOS, DIGSCN, FMTERR
        EXTERN  LITFLS, FBSP, SFDP, SFEXP, SFST10, LBRAC, RBRAC
        EXTERN  IOBTBL
        EXTERN  COUNTT, CTR, EFLDFG, FLSCHR, LAST, LENG, NESTB
        EXTERN  NSTFLS, STRPTR, TMPLOC
;
        INTERN  FBSC03, FBSC05, FBSC07, FBSC09, FBSC10, FBSC11
        INTERN  FBSC12, FBSC14, FBSC15, FBSC18, FBSC19, FBSC21
        INTERN  FBSC23
;
;*****************************************************************
        SUBTTL  FRMSCN  CHART R51
        DCL     FRMSCN
;*****************************************************************
;
;  R51.1
        CALL IFINIT                     ;R53
;
;  R51.2
FSCN02:
;
;  R51.3
        OSKIPN  ,NXTCHR
        JRST    FSCN05
;
;  R51.4
        OSETZM  ,NXTCHR
        JRST    FSCN06
;
;  R51.5
FSCN05: IGC(FMTPTR,OCB)
;
;  R51.6-.6A-.6B
FSCN06: CAIL    A0,^O141        ; LOWER CASE A
        CAILE   A0,^O172        ; LOWER CASE Z
        SKIPA
        SUBI    A0,^O40
        CAILE   STAGE,12
        JRST    @.-10(STAGE)
        JUMPL   STAGE,FSCN07            ;STAGE=-1
        JRST    FSCN08                  ;STAGE=0-12
        FSCN09                          ;STAGE=13
        FSCN10                          ;STAGE=14
        FSCN11                          ;STAGE=15
        0
        FSCN12                          ;STAGE=17
        FSCN13                          ;STAGE=18
        FSCN14                          ;STAGE=19
        0
        FSCN15                          ;STAGE=21
;
;*****************************************************************
;  R51.7  FRMBSC
FSCN07:
;*****************************************************************
;
;  R51.7.1
        SETZ    A1,
;
;  R51.7.2
        JUMPE   A0,FBSC20               ;JUMP IF EOS
        CAIL    A0,^O40                 ;ASCII BLANK
        CAILE   A0,^O135                ;ASCII RIGHT BRACKET
        JRST    FBSC23
        HRRZ    R5,IOBTBL-^O40(A0)
       JRST    @R5
;
;  R51.7.3 - .4
FBSC03: MOVEI   STAGE,17
        CALL    IFBNL                   ;R56
        JRST    FBSC$$
;
;  R51.7.5 - .6
FBSC05: MOVEI   STAGE,14
        CALL    IFBNL                   ;R56
        JRST    FBSC$$
;
;  R51.7.7 - .8
FBSC07: MOVEI   STAGE,21
        CALL    IFBNL                   ;R56
        JRST    FBSC$$
;
;*****************************************************************
;  R51.7.9   FRMLIT
FBSC09:
;*****************************************************************
;
;  R51.7.9.1
        CALL    IFBLIT                  ;R55
;
;  R51.7.9.2
        MOVEM   A0,FLSCHR
;
;  R51.7.9.3
        MOVEI   R5,1
        MOVEM   R5,COUNTT
;
;  R51.7.9.4
        CALL    LITFLS                  ;R52
;
;  R51.7.9.5
        MOVEI   STAGE,20
;
;  R51.7.9.6
        CALL    IFCLIT                  ;R59
;
        JRST    FBSC$$
;
;  R51.7.10
FBSC10: CALL    DIGSCN                  ;R64
        JRST    FBSC$$
;
;*****************************************************************
;  R51.7.11  R FORM
FBSC11:
;*****************************************************************
;
;  R51.7.11.1
        OSKIPE  ,NUMFLG
        JRST    RFRM03
;
;  R51.7.11.2
        MOVEI   STAGE,19
        JRST    RFRM$$
;
;  R51.7.11.3
RFRM03: OSKIPN  ,NUMFLD
        JRST    RFRM05
;
;  R51.7.11.4
        CALL    FMTERR,<RSJMSG+^D8>
;
;  R51.7.11.5
RFRM05: OSETZM  ,NUMFLG
;
RFRM$$: JRST    FBSC$$
;
;  R51.7.12 - .13 - .13A
FBSC12: MOVEI   STAGE,13
        CALL    IFMDP                   ;R58
        OMOVE   R5,FMTPTR
        OMOVEM   R5,FLDBGN
        JRST    FBSC$$
;
;*****************************************************************
;  R51.7.14  FCSF
FBSC14:
;*****************************************************************
;
;  R51.7.14.1
        OSKIPE  ,NUMFLG
        JRST    FCSF05
;
;  R51.7.14.2
        MOVEI   R5,1
        OMOVEM   R5,LFP
;
;  R51.7.14.3 - .4
        MOVEM   R5,EFLDFG
        MOVEI   STAGE,22
        JRST    FCSF10
;
;  R51.7.14.5
FCSF05: OSKIPE  ,NUMFLD
        JRST    FCSF07
;
;  R51.7.14.6
        CALL    IFINIT                  ;R53
        JRST    FCSF10
;
;  R51.7.14.7
FCSF07: OMOVE   R5,NUMFLD
        OMOVEM   R5,LFP
;
;  R51.7.14.8
        MOVEI   R5,1
        MOVEM   R5,EFLDFG
;
;  R51.7.14.9
        MOVEI   STAGE,22
;
;  R51.7.14.10 - .11
FCSF10: OSETZM  ,NUMFLG
        OSETZM  ,NUMFLD
;
;  RETURN
        JRST    FBSC$$
;
;  R51.7.15 - .17
FBSC15: MOVEI   R5,1
        OMOVEM  R5,NXTCHR
        SETZ    STAGE,
        CALL    FBSP                    ;R54
        JRST    FBSC$$
;
;**************************************************************
;  R51.7.18  LPAR
FBSC18: 
;**************************************************************
;
;  R51.7.18.1
        OAOS    R5,NESTP
;
;  R51.7.18.2
        CAIG    R5,3
        JRST    LPAR04
;
;  R51.7.18.3
        CALL    FMTERR,<RSJMSG+^D2>
;
;  R51.7.18.4
LPAR04: OMOVE   R5,FMTPTR
        IMOVEM(R5,BEGLVP,NESTP) ;R5=>BEGLVP(NESTP)
;
;  R51.7.18.5
        OSKIPG  ,NUMFLG
        JRST    LPAR12
;
;  R51.7.18.6
        OSKIPE  ,NUMFLD
        JRST    LPAR09
;
;  R51.7.18.7
        OMOVE   R5,NESTP
        MOVEM   R5,NSTFLS
;
;**************************************************************
;  R51.7.18.8  PARFLS
;**************************************************************
;
;  R51.7.18.8.1
        MOVEI   CONT,1
;
;  R51.7.18.8.2
PFLS02:
;
;  R51.7.18.8.3
        IGC(FMTPTR,OCB)
;
;  R51.7.18.8.4
        CAIN    A0,"("
        JRST    PFLS05
        CAIN    A0,")"
        JRST    PFLS06
        JUMPE   A0,PFLS10               ;JUMP IF EOS
        JRST    PFLS11
;
;  R51.7.18.8.5
PFLS05: AOS     ,NSTFLS
        JRST    PFLS11
;
;  R51.7.18.8.6
PFLS06: SOS     ,NSTFLS
;
;  R51.7.18.8.7
        OMOVE   R5,NESTP
        SUBI    R5,1
        CAME    R5,NSTFLS
        JRST    PFLS11
;
;  R51.7.18.8.8
        OSOS    ,NESTP
;
;  R51.7.18.8.9
        SETZ    CONT,
        JRST    PFLS11
;
;  R51.7.18.8.10
PFLS10: CALL    FMTERR,<RSJMSG+^D4>
;
;  R51.7.18.8.11
PFLS11: JUMPN   CONT,PFLS02             ;JUMP IF CONT=1
;
;  RETURN
        JRST    LPAR11
;
;  R51.7.18.9
LPAR09: OMOVE   R5,NUMFLD
        IMOVEM(R5,RPEATP,NESTP) ;R5=>RPEATP(NESTP)
;
;  R51.7.18.10
        OSETZM  ,NUMFLD
;
;  R51.7.18.11
LPAR11: OSETZM  NUMFLG
        JRST    LPAR$$
;
;  R51.7.18.12
LPAR12: MOVEI   R5,1
        IMOVEM(R5,RPEATP,NESTP) ;R5=>RPEATP(NESTP)
;
LPAR$$: JRST    FBSC$$
;
;**************************************************************
;  R51.7.19  RTPAR
FBSC19:
;**************************************************************
;
;  R51.7.19.1
        OSKIPN  ,NUMFLG
        JRST    RTPR03
;
;  R51.7.19.2
        CALL    FMTERR,<RSJMSG+^D5>
;
;  R51.7.19.3
RTPR03: ISOS(R5,RPEATP,NESTP)           ;RPEATP(NESTP)-1 => R5
;
;  R51.7.19.4
        SKIPLE  ,R5                     ;SKIP IF RPEATP(NESTP)<=0
        JRST    RTPR11
;
;  R51.7.19.5
        OSOS    ,NESTP
;
;  R51.7.19.6 - .8
;
;  R51.7.19.9
        SETO    R5,
        OCAMG   R5,NESTP
        JRST    RTPR$$
;
;  R51.7.19.10
        CALL    FMTERR,<RSJMSG+^D3>
;
;  R51.7.19.11
RTPR11: IMOVE(R5,BEGLVP,NESTP)  ;R5<=BEGLVP(NESTP)
        OMOVEM  R5,FMTPTR
;
RTPR$$: JRST    FBSC$$
;
;  R51.7.20
FBSC20: CALL    IFEOS                   ;R62
        JRST    FBSC$$
;
;  R51.7.21 - .22
FBSC21: OSKIPN  ,NUMFLG
        JRST    FBSC$$
        CALL    FMTERR,<RSJMSG+^D5>
;
;  R51.7.23
FBSC23: CALL    FMTERR,<RSJMSG+^D1>
;
FBSC$$: JRST    FSCN16
;
;**************************************************************
;  R51.8  FRMSPF
FSCN08:
;**************************************************************
;
;  R51.8.1  SETLEN
;
;  R51.8.1.1 - .3
        MOVEI   R5,1
        OSKIPE  ,NUMFLG
        OMOVE   R5,NUMFLD
        MOVEM   R5,LENG
;
;  RETURN
;  R51.8.2
        SETZ    I,
        CAIL    A0,^O40
        CAILE   A0,^O135
        JRST    FSPF04
        HLRE    I,IOBTBL-^O40(A0)       ;SET INDEX I
;
;  R51.8.3
        JUMPG   I,FSPF05                ;JUMP IF I NOT = 0
;
;**************************************************************
;  R51.8.4   PROCESS INDEPENDENT OF STAGE (PIOS)
FSPF04:
;**************************************************************
;
;  R51.8.4.1
        JUMPE   A0,PIOS12               ;EOS
        MOVMS   ,I                      ;FEATURE TO SPEED INDEXING
        JRST    @.+1(I)                 ;THIS IS THE 'DO CASE A0'
        PIOS14                          ;OTHER
        PIOS02                          ;DIGIT
        PIOS04                          ;B,COMMA, OR BACKSLASH
        PIOS08                          ;' OR "
        PIOS10                          ;[
        PIOS11                          ;]
        PIOS12                          ;(,),/,SPACE
;
;  R51.8.4.2
PIOS02: OAOS    ,NUMFLG
        OMOVE   R5,NUMFLD
        IMULI   R5,10
;
;  R51.8.4.3
        ADD     R5,A0
        SUBI    R5,^O60
        OMOVEM  R5,NUMFLD
        JRST    PIOS$$
;
;  R51.8.4.4 - .7
PIOS04: OSETZM  ,NUMFLG
        OSETZM  ,NUMFLD
        CAIN    A0,","                  ;COMMA
        SKIPN   ,LENG
        JRST    PIOS$$
        MOVEI   R5,1
        OMOVEM   R5,NUMONY
        JRST    PIOS$$
;
;************************************************************
; R51.8.4.8     SFLIT
PIOS08:
;************************************************************
;
; R51.8.4.8.1-.4
        MOVEM   A0,FLSCHR
        SETZM   ,COUNTT
        MOVE    R5,LENG
        CAIG    R5,1
        JRST    SFLT05
        OMOVE   R5,FMTPTR
        MOVEM   R5,STRPTR
;
; R51.8.4.8.5
SFLT05:
;
; R51.8.4.8.6
        CALL    LITFLS  ;R52
;
; R51.8.4.8.7
        SOS     ,LENG
;
; R51.8.4.8.8
        SKIPG   ,LENG
        JRST    SFLT10
;
; R51.8.4.8.9
        MOVE    R5,STRPTR
        OMOVEM  R5,FMTPTR
;
; R51.8.4.8.10
SFLT10: MOVE    R5,LENG
        JUMPG   R5,SFLT05
;
; RETURN
; R51.8.4.9
        OSETZM  ,NUMFLG
        OSETZM  ,NUMFLD
        JRST    PIOS$$
;
; R51.8.4.10
PIOS10: CALL    LBRAC   ;R88
        JRST    PIOS$$
;
; R51.8.4.11
PIOS11: CALL    RBRAC   ;R89
        JRST    PIOS$$
;
;************************************************************
;R51.8.4.12     FCSPF
PIOS12:
;************************************************************
;
; R51.8.4.12.1
        SETO    R5,
        CAMN    R5,NESTB
        JRST    CSPF2A
;
; R51.8.4.12.2
        CALL    FMTERR,<RSJMSG+^D16>
;
CSPF2A: OSKIPE  ,NUMFLG
        CAIE    A0,^O40
        JRST    CSPF03
        CALL    FMTERR,<RSJMSG+^D5>
;
; R51.8.4.12.3
CSPF03: CAIG    STAGE,1
        JRST    CSPF04
        CAIG    STAGE,10
        JRST    CSPF05
        CAIN    STAGE,11
        JRST    CSPF09
        JRST    CSPF10  ;STAGE=12
;
; R51.8.4.12.4
CSPF04: CALL    FMTERR,<RSJMSG+^D24>
;
; R51.8.4.12.5
CSPF05: CAIE    STAGE,2
        JRST    CSPF08
;
; R51.8.4.12.6
        OMOVE    R5,LIP
        CAIE    R5,1
        JRST    CSPF08
        MOVE    R5,LAST
        CAIL    R5,1
        CAILE   R5,6
        JRST    CSPF08
;
; R51.8.4.12.7
        CALL    FMTERR,<RSJMSG+^D24>
;
;************************************************************
; R51.8.4.12.8  CLSNUM
CSPF08:
;************************************************************
;
;************************************************************
;R51.8.4.12.8.1-.5      CKPFLD
;************************************************************
;
; R51.8.4.12.8.5.1
        OMOVE    R5,PFLG
        JUMPE   R5,CKPF$$
        CAIE    R5,PFLG4+PFLG6
        CAIN    R5,PFLG2+PFLG6
        JRST    CKPF$$
        CAIN    R5,PFLG1+PFLG6
        JRST    CKPF$$
        CAIN    R5,PFLG2
        JRST    CKPF02
        CAIN    R5,PFLG5
        JRST    CKPF04
        CAIN    R5,PFLG2+PFLG3
        JRST    CKPF06
        JRST    CKPF08  ;OTHER
;
; R51.8.4.12.8.5.2
CKPF02: SETO    R5,
        OCAMN    R5,LFP
        OCAME    R5,LEP
        JRST    CKPF03
        OMOVE    R5,FLFCTR+1
        OCAMN    R5,LIP
        JRST    CKPF$$
;
; R51.8.4.12.8.5.3
CKPF03: CALL    FMTERR,<RSJMSG+^D25>
;
; R51.8.4.12.8.5.4
CKPF04: OMOVE    R5,FLFCTR+2
        OCAMN    R5,LEP
        JRST    CKPF$$
;
; R51.8.4.12.8.5.5
        CALL    FMTERR,<RSJMSG+^D25>
;
; R51.8.4.12.8.5.6
CKPF06: OSKPGE  ,LEP    ;SKIP IF LEP#-1
        JRST    CKPF$$
;
; R51.8.4.12.8.5.7
        CALL    FMTERR,<RSJMSG+^D25>
;
; R51.8.4.12.8.5.8
CKPF08: CALL    FMTERR,<RSJMSG+^D25>
;
CKPF$$:
;
; R51.8.4.12.8.6
        OAOS    ,NULFMT
;
; R51.8.4.12.8.7
        MOVEI   R5,1
        MOVEM   R5,EFLDFG
;
; R51.8.4.12.8.8
        OSKPGE  ,LFP
        OSKIPL   ,LEP
        JRST    .+2
        JRST    CLSN12
;
; R51.8.4.12.8.9
        MOVEI   R5,1
        OMOVEM   R5,NUMONY
;
; R51.8.4.12.8.10
        OSKIPE   ,LEP
        JRST    CLSN12
;
; R51.8.4.12.8.11
        CALL    FMTERR,<RSJMSG+^D23>
;
;************************************************************
; R51.8.4.12.8.12       CHECK FLOATING FIELDS (CHFF)
CLSN12:
;************************************************************
;
; R51.8.4.12.8.12.1
        OMOVE    R5,FLFCTR+1
        CAIE    R5,1
        JRST    CHFF03
        OMOVE   R5,FLFCHR+1
        CAIN    R5,"Q"
        JRST    CHFF03
;
; R51.8.4.12.8.12.2
        CALL    FMTERR,<RSJMSG+^D21>
;
; R51.8.4.12.8.12.3
CHFF03: OMOVE    R5,FLFCTR+2
        CAIE    R5,1
        JRST    CHFF$$
        OMOVE   R5,FLFCHR+2
        CAIN    R5,"Q"
        JRST    CHFF$$
;
; R51.8.4.12.8.12.4
        CALL    FMTERR,<RSJMSG+^D23>
;
CHFF$$:
;
; RETURN
CLSN$$: JRST    CSPF11
;
;************************************************************
; R51.8.4.12.9  CLSSTR
CSPF09:
;************************************************************
;
; R51.8.4.12.9.1
        OMOVE    R5,NUMONY
        CAIE    R5,1
        JRST    CSTR03
;
; R51.8.4.12.9.2
        CALL    FMTERR,<RSJMSG+^D20>
;
; R51.8.4.12.9.3
CSTR03: OAOS    ,NULFMT
;
; R51.8.4.12.9.4
        MOVEI   R5,1
        MOVEM   R5,EFLDFG
;
        JRST    CSPF11
;
;************************************************************
; R51.8.4.12.10 CLSIC
CSPF10:
;
;************************************************************
;
; R51.8.4.12.10.1-.2
        OAOS    ,NULFMT
        MOVEI   R5,1
        MOVEM   R5,EFLDFG
;
;       RETURN
;
; R51.8.4.12.11
CSPF11: MOVEI   R5,1
        OMOVEM  R5,NXTCHR
;
; RETURN
        JRST    PIOS$$
;
; R51.8.4.13
;
; R51.8.4.14
PIOS14: CALL    FMTERR,<RSJMSG+^D1>
;
; RETURN
PIOS$$: JRST    FSPF$$
;
;************************************************************
; R51.8.5       PROCESS BY STAGE (PBS)
FSPF05:
;************************************************************
;
; R51.8.5.1
        SKIPN   ,LENG
        JRST    PBS17
;
; R51.8.5.2
        JRST    @.+1(STAGE)
        PBS03                   ;STAGE=0
        PBS04                   ;STAGE=1
        PBS05                   ;STAGE=2
        PBS06                   ;STAGE=3
        PBS07                   ;STAGE=4
        PBS08                   ;STAGE=5
        PBS09                   ;STAGE=6
        PBS10                   ;STAGE=7
        PBS11                   ;STAGE=8
        PBS12                   ;STAGE=9
        PBS13                   ;STAGE=10
        PBS14                   ;STAGE=11
        PBS15                   ;STAGE=12
;
;************************************************************
; R51.8.5.3     FSTG0
PBS03:
;************************************************************
;
; R51.8.5.3.1
        CAIG    I,6
        JRST    FS0G02  ;I=1-6
        CAIG    I,10
        JRST    FS0G03
        CAIG    I,12
        JRST    FS0G10
        CAIG    I,14
        JRST    FS0G11
        JRST    FS0G12  ;I=15-22
;
;************************************************************
; R51.8.5.3.2   STG0ST
FS0G02:
;************************************************************
;
; R51.8.5.3.2.1-.2
        MOVEI   R5,1
        OMOVEM   R5,NUMONY
        CAME    R5,LENG
        JRST    S0ST07
;
; R51.8.5.3.2.3-.4
        MOVEI   STAGE,1
        MOVEM   R5,CTR(I)
;
; R51.8.5.3.2.5-.6
        CAIE    I,6
        JRST    S0ST6A
        MOVEI   R5,PFLG1
        OORM     R5,PFLG
;
; R51.8.5.3.2.6A
S0ST6A: OMOVE   R5,FMTPTR
        MOVEM   R5,TMPLOC
        JRST    S0ST$$
;
; R51.8.5.3.2.7-.8
S0ST07: MOVEI   STAGE,2
        MOVE    R5,[XWD CTR+1,CTR+2]
        SETZM   ,CTR+1
        BLT     R5,CTR+6
;
; R51.8.5.3.2.9
        MOVE    R5,LENG
        OADDM    R5,LIP
;
; R51.8.5.3.2.10
        OMOVE   R6,FMTPTR
        OMOVEM   R6,FLFBGN+1
;
; R51.8.5.3.2.11
        OMOVEM   A0,FLFCHR+1
        OMOVEM   R5,FLFCTR+1
;
; R51.8.5.3.2.12-.13
        CAIE    I,6
        JRST    S0ST$$
        MOVEI   R5,PFLG2
        OORM     R5,PFLG
;
S0ST$$: JRST    FS0G18
;
; R51.8.5.3.3
FS0G03: MOVE    R5,LENG
        OADDM    R5,LIP
;
; R51.8.5.3.4
        CAILE   I,8     ;SKIP IF I=7 OR 8
        JRST    FS0G06
;
; R51.8.5.3.5
        MOVEI   STAGE,3
        JRST    FS0G18
;
; R51.8.5.3.6
FS0G06: CAIE    I,9
        JRST    FS0G8A
;
; R51.8.5.3.7
        MOVEI   R5,1
        OMOVEM   R5,OUTONY
;
;*************************************************************
; R51.8.5.3.8   MARKFF0
;*************************************************************
;
; R51.8.5.3.8.1
        MOVE    R5,FMTPTR
        OMOVEM   R5,FLFBGN+1
;
; R51.8.5.3.8.2
        OMOVEM   A0,FLFCHR+1
        MOVE    R5,LENG
        OMOVEM   R5,FLFCTR+1
; R51.8.5.3.8A
FS0G8A: MOVEI   R5,1
        OMOVEM   R5,NUMONY
;
